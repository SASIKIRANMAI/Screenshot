    "\n",
    "import random\n",
    "\n",
    "import time\n",
    "\n",
    "while True:\n",
    "\t\n",
    "\trandom_time = random.randint(1, 5)\n",
    "\n",
    "\t\n",
    "\ttime.sleep(random_time)\n",
    "\n",
    "\t\n",
    "\tsnapshot = ImageGrab.grab()\n",
    "\n",
    "\t\n",
    "\tfile_name = str(time.time())+\".png\"\n",
    "\tsnapshot.save(file_name)"
   ]
